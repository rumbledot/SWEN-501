package ac.bram.RecursionTest;

import java.util.ArrayList;

public class Tree {

	ArrayList<Tree> children = new ArrayList<>();
	Tree t;
	String prefix;
	
	public Tree(Tree t, String prefix)
	{
		this.t = t;
		this.prefix = prefix;
		children.add(t);
	}
	
	public ArrayList<Tree> getChildren() {
		return children;
	}
}
