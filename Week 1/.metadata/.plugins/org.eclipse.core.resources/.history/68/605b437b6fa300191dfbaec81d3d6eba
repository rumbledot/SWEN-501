package ac.swen501.bram.BugWorld;

public class Thing {
	private int x;
	private int y;
	
	public Thing(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	public int x() {
		return this.x;
	}
	
	public int y() {
		return this.y;
	}
	
	public void setX(int x) {
		this.x = x;
	}
	
	public int setY(int y) {
		this.y = y;
	}
}
