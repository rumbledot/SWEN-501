package ac.swen501.bram.BugWorld;

public class Thing {
	
	private int x, y;
	
	public Thing(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	public int x() {
		return this.x;
	}
	
	public int y() {
		return this.y;
	}
	
	public void setX(int xx) {
		this.x = xx;
	}
	
	public void setY(int yy) {
		this.y = yy;
	}

	public void doSomething() {
		
	}
}
